{
	"id": "embedding_disabled",
	"description": "Tests chunking without embeddings - chunks should not have embedding vectors",
	"tags": ["embeddings", "chunking"],
	"document": {
		"path": "pdf/fake_memo.pdf"
	},
	"extraction": {
		"method": "sync",
		"input_type": "file",
		"config": {
			"chunking": {
				"max_chars": 500,
				"max_overlap": 50
			}
		}
	},
	"assertions": {
		"expected_mime": "application/pdf",
		"min_content_length": 10,
		"chunks": {
			"min_count": 1,
			"each_has_content": true,
			"each_has_embedding": false
		}
	}
}
