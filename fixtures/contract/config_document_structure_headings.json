{
	"id": "config_document_structure_headings",
	"description": "Tests document structure extraction with heading nodes on a DOCX",
	"tags": ["contract", "document_structure"],
	"document": {
		"path": "docx/unit_test_headers.docx"
	},
	"extraction": {
		"method": "sync",
		"input_type": "file",
		"config": {
			"include_document_structure": true
		}
	},
	"assertions": {
		"expected_mime": ["application/vnd.openxmlformats-officedocument.wordprocessingml.document"],
		"document": {
			"has_document": true,
			"min_node_count": 1,
			"node_types_include": ["heading", "paragraph"]
		}
	},
	"skip": {
		"requires_feature": ["office"]
	}
}
